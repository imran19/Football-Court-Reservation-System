@model IEnumerable<FCRS.Models.Reservation>




<head>
</head>
<body style="width: 100%; height: 100%;">



</body>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User.Firstname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ReservationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RequestStop)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User.Firstname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ReservationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequestStart)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RequestStop)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        
    </tr>
}

</table>

<style>
    .button {
        position: relative;
        background-color: #4CAF50;
        border: none;
        font-size: 12px;
        color: #FFFFFF;
        padding: 10px;
        width: 90px;
        text-align: center;
        -webkit-transition-duration: 0.4s; /* Safari */
        transition-duration: 0.4s;
        text-decoration: none;
        overflow: hidden;
        cursor: pointer;
    }

        .button:after {
            content: "";
            background: #90EE90;
            display: block;
            position: absolute;
            padding-top: 300%;
            padding-left: 350%;
            margin-left: -20px !important;
            margin-top: -120%;
            opacity: 0;
        }

        .button:active:after {
            padding: 0;
            margin: 0;
            opacity: 1;
            transition: 0s;
        }

    body {
        background-image: url('/Content/Images/FudbalSLikaPNG.png');
        background-attachment: fixed;
        background-position: center;
        background-repeat: no-repeat;
    }

    .table {
        background-color: white;
        box-shadow: 0 0 20px 0 rgba(0, 0, 0, 0.2), 0 5px 5px 0 rgba(0, 0, 0, 0.24);
    }
</style>
